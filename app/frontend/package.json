{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "stage": "NODE_ENV=stage vite --mode stage",
    "dev": "NODE_ENV=development vite --mode development",
    "prod": "NODE_ENV=production vite --mode production",
    "build:stage": "tsc && NODE_ENV=stage vite build --mode stage",
    "build:dev": "tsc && NODE_ENV=development vite build --mode development",
    "build:prod": "tsc && NODE_ENV=production vite build --mode production",
    "build": "npm run build:prod",
    "preview": "vite preview"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.1.0",
    "@azure/msal-react": "^2.0.4",
    "@fluentui/react": "^8.112.3",
    "@fluentui/react-components": "^9.35.0",
    "@fluentui/react-icons": "^2.0.220",
    "@react-spring/web": "^9.7.3",
    "dompurify": "^3.0.6",
    "dotenv": "^16.3.1",
    "ndjson-readablestream": "^1.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "scheduler": "^0.20.2"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.3",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vitejs/plugin-react": "^4.1.0",
    "prettier": "^3.0.3",
    "vite": "^4.4.11"
  }
}
